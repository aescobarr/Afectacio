<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//hibernate/hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cat.creaf.afectaciobosc.model.Provincia" table="Provincies" schema="AfectacioBosc">
      <cache usage="read-only"></cache>
    <!-- A 32 hex character is our surrogate key. It's automatically
                generated by Hibernate with the UUID pattern. -->
    <id name="idString" type="string" unsaved-value="null">
      <column name="IdProvincia" not-null="true" sql-type="char(100)"/>
      <generator class="GUtilDAOHibernate.GeneradorIDs"/>
    </id>
    <property name="nom">
      <column length="255" name="NomProvincia"/>
    </property>
  </class>
</hibernate-mapping>
