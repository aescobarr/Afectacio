<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//hibernate/hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cat.creaf.afectaciobosc.model.Comarca" table="Comarques" schema="AfectacioBosc">
      <cache usage="read-only"></cache>
    <!-- A 32 hex character is our surrogate key. It's automatically
                generated by Hibernate with the UUID pattern. -->
    <id name="idString" type="string" unsaved-value="null">
      <column name="IdComarca" not-null="true" sql-type="char(100)"/>
      <generator class="GUtilDAOHibernate.GeneradorIDs"/>
    </id>
    <property name="nom">
      <column length="255" name="NomComarca"/>
    </property>
    <bag name="provincies" schema="AfectacioBosc" table="ComarquesProvincies">
        <key column="IdComarca"/>
        <many-to-many class="cat.creaf.afectaciobosc.model.Provincia" column="IdProvincia"/>
    </bag>
    <many-to-one name="geometria" column="IdGeometria" class="cat.creaf.sipan.MGeometriaWrapper.GeometriaWrapper" cascade="all"/>
  </class>
</hibernate-mapping>
