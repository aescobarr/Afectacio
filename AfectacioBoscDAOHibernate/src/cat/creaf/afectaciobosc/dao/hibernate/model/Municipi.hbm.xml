<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//hibernate/hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="cat.creaf.afectaciobosc.model.Municipi" table="Municipis" schema="AfectacioBosc">
      <cache usage="read-only"></cache>
    <!-- A 32 hex character is our surrogate key. It's automatically
                generated by Hibernate with the UUID pattern. -->
    <id name="idString" type="string" unsaved-value="null">
      <column name="IDMUNICIPI" not-null="true" sql-type="char(100)"/>
      <generator class="GUtilDAOHibernate.GeneradorIDs"/>
    </id>
    <property name="nom">
      <column length="255" name="NomMunicipi" />
    </property>
    <many-to-one name="comarca" column="idComarca" class="cat.creaf.afectaciobosc.model.Comarca"/>
    <many-to-one name="provincia" column="idProvincia" class="cat.creaf.afectaciobosc.model.Provincia"/>
    <many-to-one name="geometria" column="IdGeometria" class="cat.creaf.sipan.MGeometriaWrapper.GeometriaWrapper" cascade="all"/>
  </class>
</hibernate-mapping>
